<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
<item>
    <rotate
        android:fromDegrees="315">
        <shape
            android:shape="rectangle">

            <corners
                android:topRightRadius="1dp"
                android:radius="40dp" />
            <size
                android:width="82dp"
                android:height="82dp" />
            <stroke
                android:width="1dp"
                android:color="#FFFFFF" />
        </shape>
    </rotate>
</item>

</layer-list>